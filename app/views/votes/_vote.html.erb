<div class="span0 well text-center">
	<%= link_to vote_post_path(type, vote: true), method: 'post' do %>
		<i class="icon-arrow-up"></i>
	<% end %>
	<br>
	
	<%= type.total_votes %> votes
	<br>
	<%= link_to vote_post_path(type, vote: false), method: 'post' do %>
		<i class="icon-arrow-down"></i>
	<% end %>
</div>


